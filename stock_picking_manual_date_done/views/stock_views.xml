<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_picking_form_date_done" model="ir.ui.view">
        <field name="name">view.picking.form.date.done</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <field name="date_done" position="attributes">
                <attribute name="attrs"></attribute>
            </field>
        </field>
    </record>

    <record id="view_stock_move_line_operation_date_done" model="ir.ui.view">
        <field name="name">stock.move.line.operations.date.done</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_stock_move_line_operation_tree"/>
        <field name="arch" type="xml">
            <field name="qty_done" position="before">
                <field name="date"/>
            </field>
        </field>
    </record>
</odoo>