<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data noupdate="1">
    <!-- Sequence type for product categories -->
    <record id="seq_type_product_category" model="ir.sequence.type">
      <field name="name">Product Category</field>
      <field name="code">product.category</field>
    </record>

    <!-- Default sequence for product -->
    <record id="seq_product_category" model="ir.sequence">
      <field name="name">Default Product Sequence</field>
      <field name="code">product.category</field>
      <field name="prefix">PP</field>
      <field name="padding">6</field>
      <field name="company_id" eval="False"/>
    </record>

  </data>
</openerp>
